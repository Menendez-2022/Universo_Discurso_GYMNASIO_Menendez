

ALTER TABLE EMPLEADO ADD CONSTRAINT TIPO_EMPLEADO_fk FOREIGN KEY (TIPO_ID_EMPLEADO )
REFERENCES EMPLEADO (EMPLEADO_ID) MATCH FULL
ON DELETE RESTRICT ON UPDATE CASCADE;
